<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Amazon.com</ShortName>
<Description>Amazon.com Search</Description>
<InputEncoding>ISO-8859-1</InputEncoding>
<Image width="16" height="16">data:image/png;base64,</Image>
<Image width="64" height="64">data:image/png;base64,</Image>
<Url type="text/html" method="GET" template="https://www.amazon.com/exec/obidos/external-search/">
  <Param name="field-keywords" value="{searchTerms}"/>
  <Param name="mode" value="blended"/>
</Url>
<SearchForm>https://www.amazon.com/</SearchForm>
</SearchPlugin>
